# MIW Railway Environment Variables
# Copy this file to .env and fill in your actual values

# Database Configuration
DB_HOST=localhost
DB_PORT=3306
DB_NAME=miw_db
DB_USER=root
DB_PASS=

# Email Configuration
SMTP_HOST=smtp.gmail.com
SMTP_USERNAME=your-email@gmail.com
SMTP_PASSWORD=your-app-password
SMTP_PORT=587
SMTP_ENCRYPTION=tls

# Railway Configuration (for production)
RAILWAY_ENVIRONMENT=production
RAILWAY_PROJECT_ID=your-project-id
MYSQLDATABASE_HOST=your-railway-host
MYSQLDATABASE_PORT=3306
MYSQLDATABASE_DATABASE=railway
MYSQLDATABASE_USER=root
MYSQLDATABASE_PASSWORD=your-railway-password
